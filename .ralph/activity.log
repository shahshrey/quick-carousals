  bucket_id = 'exports' AND
  auth.uid()::text = (storage.foldername(name))[1]
);

-- Allow users to delete their own exports
CREATE POLICY "Users can delete their own exports"
ON storage.objects FOR DELETE
TO authenticated
USING (
  bucket_id = 'exports' AND
  auth.uid()::text = (storage.foldername(name))[1]
);
EOF
